    <main class="container">
      <section id="quiz" class="mb-2">
        <div class="text-center mb-1">
          <h1>Scenic Quiz Answer The Legend Of Neverlands</h1>
          <div adsense="fill"></div>
        </div>
        <div class="mb-2">
          <form class="form-floating" id="nosubmit">
            <input type="text" class="form-control" id="quizTerm" placeholder="Liz Went Missing at 5" value="">
            <label for="quizTerm">search question here</label>
            <div class="form-check">
              <input class="form-check-input" type="checkbox" value="" id="showOnly">
              <label class="form-check-label" for="showOnly">
                Show only
                <b>(O)</b>
              </label>
            </div>
            <small id="search-questionsHelpBlock" class="form-text text-muted">
              Quiz The Legend Of Neverland Last updated {{ page.updated }} -
              <a href="#Changelog">changelog</a>
            </small>
          </form>
        </div>
        <div id="qresult"></div>
      </section>
      <section id="add-quiz-section" class="mb-2">
        <form
          action="https://backend.webmanajemen.com/tlon/quiz.php"
          method="post"
          id="addQuiz"
          target="_blank"
          name="form_add_quiz"
          class="mb-4 text-center">
          <input type="hidden" name="add" value="add">
          <div class="row">
            <div class="col-md-6">
              <label for="newQ">Add new question</label>
              <textarea type="text" id="newQ" placeholder="add new question" name="q" class="form-control"></textarea>
            </div>
            <div class="col-md-4">
              <div class="mb-2">
                Answer of new question (pick one)
              </div>
              <div class="row">
                <div class="col">
                  <div class="form-check">
                    <input type="radio" id="customRadioInline1" name="a" name="q" class="form-check-input" value="O">
                    <label class="form-check-label" for="customRadioInline1">(O)</label>
                  </div>
                </div>
                <div class="col">
                  <div class="form-check">
                    <input type="radio" id="customRadioInline2" name="a" class="form-check-input" value="X">
                    <label class="form-check-label" for="customRadioInline2">(X)</label>
                  </div>
                </div>
              </div>
              <div id="submitter" class="mt-2">
                <button type="submit" class="btn btn-primary btn-block btn-sm">Send</button>
              </div>
            </div>
          </div>
        </form>
      </section>
